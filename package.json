{
  "name": "@beetcinema/contracts",
  "version": "1.0.0",
  "description": "Proto contracts for Beet Cinema microservices and generated TypeScript types from Protobuf definitions.",
  "scripts": {
    "generate": "protoc -I ./proto ./proto/*.proto --ts_proto_out=./gen --ts_proto_opt=nestJs=true,package=omit"
  },
  "files": [
    "proto",
    "gen"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "ts-proto": "^2.11.0"
  },
  "dependencies": {
    "@nestjs/microservices": "^11.1.12",
    "rxjs": "^7.8.2"
  }
}
